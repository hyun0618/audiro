 document.addEventListener('DOMContentLoaded', function() {
    // Í≤ÄÏÉâ Î≤ÑÌäº ÌÅ¥Î¶≠ Ïù¥Î≤§Ìä∏ Î¶¨Ïä§ÎÑà Ï∂îÍ∞Ä
    document.getElementById('btnSearch').addEventListener('click', function(event) {
        event.preventDefault(); // Ìèº Ï†úÏ∂úÏùÑ ÎßâÏäµÎãàÎã§.

        // Í≤ÄÏÉâÏñ¥ Í∞ÄÏ†∏Ïò§Í∏∞
        const query = document.getElementById('inputSearch').value;

        // Í≤ÄÏÉâÏñ¥Í∞Ä ÎπÑÏñ¥ÏûàÎäîÏßÄ ÌôïÏù∏
        if (!query.trim()) {
            alert("Í≤ÄÏÉâÏñ¥Î•º ÏûÖÎ†•ÌïòÏÑ∏Ïöî.");
            return;
        }

        // ÏÑ†ÌÉùÌïú Ïπ¥ÌÖåÍ≥†Î¶¨ Í∞í Í∞ÄÏ†∏Ïò§Í∏∞
        const category1 = document.querySelector('select[name="category1"]').value;
        const category2 = document.querySelector('select[name="category2"]').value;

        // 'Í≤ÄÏÉâ Í≤∞Í≥º' ÌÉ≠ ÌôúÏÑ±Ìôî
        const searchTabLink = document.querySelector('a[href="#search"]');
        if (searchTabLink) {
            const searchTab = new bootstrap.Tab(searchTabLink);
            searchTab.show();
        }

        // Í≤ÄÏÉâ ÏöîÏ≤≠ Îç∞Ïù¥ÌÑ∞
        const data = {
            keyword: query,
            category1: category1,
            category2: category2
        };

        console.log('Í≤ÄÏÉâ ÏöîÏ≤≠ Îç∞Ïù¥ÌÑ∞:', data);

        // AxiosÎ•º ÏÇ¨Ïö©ÌïòÏó¨ Í≤ÄÏÉâ ÏöîÏ≤≠ Î≥¥ÎÇ¥Í∏∞
        axios.post('../community/search', data)
            .then(function(response) {
                console.log('Í≤ÄÏÉâ ÏùëÎãµ Îç∞Ïù¥ÌÑ∞:', response.data);
                // Í≤ÄÏÉâ Í≤∞Í≥ºÎ•º Í∞ÄÏ†∏Ïò® ÌõÑ ÌëúÏãúÌï† HTML ÏöîÏÜå Í∞ÄÏ†∏Ïò§Í∏∞
                const divSearchResultsContainer = document.getElementById('search');
                const searchResultsContainer = document.getElementById('searchResults');
                // Í∏∞Ï°¥ Í≤ÄÏÉâ Í≤∞Í≥º Ï¥àÍ∏∞Ìôî
                searchResultsContainer.innerHTML = '';

                // Í≤ÄÏÉâ Í≤∞Í≥ºÍ∞Ä ÏóÜÎäî Í≤ΩÏö∞
                if (response.data == null || response.data.length === 0) {
                	divSearchResultsContainer.innerHTML = '<div style="padding: 100px;"><p class="text-center mb-5">Í≤ÄÏÉâ Í≤∞Í≥ºÍ∞Ä ÏóÜÏäµÎãàÎã§.</p><div>';
                    return;
                }

                // Í≤ÄÏÉâ Í≤∞Í≥ºÎ•º Î∞òÎ≥µÌïòÎ©∞ HTML ÏÉùÏÑ±
                response.data.searchPostList.forEach(post => {
                	const postDetailsPage = `../community/details?postId=\${post.postId}`;
                    const resultHTML = `
                        <tr class="table-light">
                            <td class="text-center">\${post.postId}</td>
                            <td class="text-center"> 
                                <a href="\${postDetailsPage}">\${post.title}</a>
                            </td>
                            <td class="text-center">\${post.nickname}</td>
                            <td class="text-center">\${post.good}</td>
                            <td class="text-center">\${post.createdTime}</td>
                        </tr>
                    `;
                    console.log(resultHTML);
                    searchResultsContainer.innerHTML += resultHTML;
                });
            })
            .catch(function(error) {
                console.error('Í≤ÄÏÉâ Ï§ë Ïò§Î•ò Î∞úÏÉù:', error);
            });
    });
});
</script>
